@model IEnumerable<HospitalManagementSystem.Models.Fiche>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout_Secretaire.cshtml";
}



<p>
    @Html.ActionLink("Nouveau", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Paiement)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Configuration.Libelle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LibelleActe.Libelle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Patient.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Personnel.Nom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Paiement)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Configuration.Libelle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LibelleActe.Libelle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Patient.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Personnel.Nom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
